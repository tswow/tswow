{
  "module": {
  	"type":"commonjs"
  },
  "exclude":[".*.js$","typescript2cxx","wotlkdata","addons"],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "tsx": false,
      "decorators": true,
      "dynamicImport": false
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true,
      "useDefineForClassFields": false
    },
    "target":"es2021",
    "loose":false
  },
  "sourceMaps": true
}
